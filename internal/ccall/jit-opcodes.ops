	op_def("float32_to_long") { op_values(long, float32),
				    op_intrinsic(conv) }
	op_def("float32_to_ulong") { op_values(long, float32),
				     op_intrinsic(conv) }
	op_def("check_float32_to_int") { op_values(int, float32) }
	op_def("check_float32_to_uint") { op_values(int, float32) }
