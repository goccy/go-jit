			      op_intrinsic(jit_long_neg, l_l) }
	op_def("fadd", +) { op_values(float32, float32, float32),
			    op_intrinsic(jit_float32_add, f_ff) }
	op_def("fsub", -) { op_values( float32, float32, float32),
