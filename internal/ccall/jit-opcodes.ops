	op_def("iadd_ovf_un") { op_values(int, int, int),
				op_intrinsic(jit_uint_add_ovf, i_pIII) }
	op_def("isub", -) { op_values(int, int, int),
			    op_intrinsic(jit_int_sub, i_ii) }
	op_def("isub_ovf") { op_values(int, int, int),
